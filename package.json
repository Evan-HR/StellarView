  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "density-clustering": "^1.3.0",
    "dotenv": "^8.2.0",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "express-mysql-session": "^2.1.4",
    "express-session": "^1.17.1",
    "express-validator": "^5.3.1",
    "geolib": "^3.2.1",
    "heroku-ssl-redirect": "0.0.4",
    "material-ui": "^1.0.0-beta.47",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.3",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "suncalc": "^1.8.0"
